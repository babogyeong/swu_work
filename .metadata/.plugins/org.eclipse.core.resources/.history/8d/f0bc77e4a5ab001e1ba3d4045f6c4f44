package afterclass.override;

public class Programmer extends Human_ {

	public String devLang;
	public int repoCount;
	
	@Override
	public void 자기소개하기() {
		System.out.println("이름 : "+name);
		System.out.println("나이: "+age);
		System.out.println("언어 : "+devLang);
		System.out.println("나이: "+repoCount);
		
	}
	
	public void 코딩하기() {
		System.out.println("코딩할 수 있습니다");
	}
	
}
