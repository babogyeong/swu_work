	public static void main(String[] args) {
		
		BasketBallPlayer p1 = new BasketBallPlayer();
		p1.name = "dk";
		p1.height = 202;
		p1.age = 31;
		p1.salary = 1999999;
		
		BasketBallPlayer p2 = new BasketBallPlayer();
		p2.name = "justin";
