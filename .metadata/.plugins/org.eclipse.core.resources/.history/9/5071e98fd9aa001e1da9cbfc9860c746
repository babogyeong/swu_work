package struct;

public class StudentMain {

	public static void main(String[] args) {
		
		Student s1 = new Student();
		s1.age = 21;
		s1.grade = "A";
		s1.name = "곱창전골";
		
		Student s2 = new Student();
		s2.age = 26;
		s2.grade = "b";
		s2.name = "만두전골";
		
		s1.studentInfo();
		s2.studentInfo();
		
		

	}

}
